{
  "name": "medfetch",
  "version": "0.5.0",
  "type": "module",
  "description": "sql on fhir database runner implementation",
  "scripts": {
    "dev": "pnpm run docs:dev",
    "test": "vitest run --coverage",
    "build": "pnpm --filter=block-promise run build && rm -rf dist && npx concurrently \"npx vite build\" \"npx tsc -p tsconfig.build.json && npx tsc-alias -p tsconfig.build.json\"",
    "unit": "vitest src",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "dev:codegen": "tsx src/bin/codegen.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "exports": {
    "./dialects": {
      "types": "./dist/dialects.d.ts",
      "import": "./dist/dialects.js",
      "default": "./dist/dialects.js"
    },
    "./sql": {
      "types": "./dist/sql.d.ts",
      "import": "./dist/sql.js",
      "default": "./dist/sql.js"
    },
    "./sqlite": {
      "types": "./dist/sqlite.d.ts",
      "import": {
        "default": "./dist/sqlite.browser.js"
      },
      "default": "./dist/sqlite.browser.js"
    }
  },
  "files": [
    "dist",
    "src/**/*.ts",
    "tsconfig.json",
    "bin",
    "README.md"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "@sqlite.org/sqlite-wasm": "^3.49.1-build2",
    "kysely": "^0.28.2"
  },
  "devDependencies": {
    "@hono/vite-dev-server": "^0.19.1",
    "@types/clarinet": "^0.12.3",
    "@types/fhir": "^0.0.41",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.13.1",
    "@vitest/browser": "^3.1.2",
    "@vitest/coverage-v8": "2.1.9",
    "concurrently": "^9.1.2",
    "hono": "^4.7.11",
    "inquirer": "^12.6.3",
    "inquirer-autocomplete-standalone": "^0.8.1",
    "pagefind": "^1.3.0",
    "prettier": "^3.4.2",
    "ts-morph": "^25.0.0",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.2.2",
    "vitepress": "^1.6.3",
    "vitest": "^3.1.2"
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228",
  "dependencies": {
    "@hono/node-server": "^1.14.4",
    "block-promise": "workspace:^",
    "clarinet": "^0.12.6",
    "fflate": "^0.8.2",
    "kysely-generic-sqlite": "^1.2.1"
  }
}