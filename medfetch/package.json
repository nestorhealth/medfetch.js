{
    "name": "medfetch",
    "version": "0.0.12",
    "type": "module",
    "description": "sql on fhir database runner implementation",
    "scripts": {
        "test": "vitest run --coverage",
        "build": "rm -rf dist && npx vite build && npx concurrently \"npx tsc -p tsconfig.build.json\" \"cp src/sqlite-wasm/sqlite3-wasm.d.ts dist/sqlite-wasm\"",
        "unit": "vitest src",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "default": "./dist/index.mjs"
        },
        "./sqlite-wasm": {
            "types": "./dist/sqlite-wasm/index.d.ts",
            "import": "./dist/sqlite-wasm/index.mjs",
            "default": "./dist/sqlite-wasm/index.mjs"
        }
    },
    "bin": {
        "medfetch": "./bin/cli.mjs"
    },
    "files": [
        "dist",
        "src/**/*.ts",
        "tsconfig.json",
        "bin",
        "README.md"
    ],
    "keywords": [],
    "author": "",
    "license": "ISC",
    "peerDependencies": {
        "@sqlite.org/sqlite-wasm": "^3.49.1-build2",
        "effect": "^3.14.1",
        "fhirpath": "^3.17.0"
    },
    "devDependencies": {
        "@types/clarinet": "^0.12.3",
        "@types/node": "^22.13.1",
        "@vitest/coverage-v8": "2.1.9",
        "concurrently": "^9.1.2",
        "pagefind": "^1.3.0",
        "prettier": "^3.4.2",
        "ts-morph": "^25.0.0",
        "tsup": "^8.4.0",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3",
        "vite": "^6.2.2",
        "vitest": "^2.1.8"
    },
    "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228",
    "dependencies": {
        "clarinet": "^0.12.6"
    }
}
