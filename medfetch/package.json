{
    "name": "medfetch",
    "version": "0.1.0",
    "type": "module",
    "description": "sql on fhir database runner implementation",
    "scripts": {
        "dev": "vite",
        "test": "vitest run --coverage",
        "build": "rm -rf dist && npx concurrently \"npx vite build\" \"npx tsc -p tsconfig.build.json\"",
        "unit": "vitest src",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
    },
    "exports": {
        "./static": {
            "types": "./dist/static.d.ts",
            "import": "./dist/static.js",
            "default": "./dist/static.js"
        },
        "./sql": {
            "types": "./dist/sql.d.ts",
            "import": "./dist/sql.js",
            "default": "./dist/sql.js"
        },
        "./sqlite-wasm": {
            "types": "./dist/sqlite-wasm.d.ts",
            "import": "./dist/sqlite-wasm.js",
            "default": "./dist/sqlite-wasm.js"
        }
    },
    "files": [
        "dist",
        "src/**/*.ts",
        "tsconfig.json",
        "bin",
        "README.md"
    ],
    "keywords": [],
    "author": "",
    "license": "ISC",
    "peerDependencies": {
        "@sqlite.org/sqlite-wasm": "^3.49.1-build2",
        "fhirpath": "^3.17.0"
    },
    "devDependencies": {
        "@types/clarinet": "^0.12.3",
        "@types/fhir": "^0.0.41",
        "@types/node": "^22.13.1",
        "@vitest/browser": "^3.1.2",
        "@vitest/coverage-v8": "2.1.9",
        "concurrently": "^9.1.2",
        "pagefind": "^1.3.0",
        "prettier": "^3.4.2",
        "ts-morph": "^25.0.0",
        "tsup": "^8.4.0",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3",
        "vite": "^6.2.2",
        "vitest": "^3.1.2"
    },
    "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228",
    "dependencies": {
        "clarinet": "^0.12.6",
        "effect": "^3.14.1",
        "fflate": "^0.8.2",
        "kysely": "^0.28.2",
        "kysely-generic-sqlite": "^1.2.1",
        "kysely-wasm": "^1.2.1",
        "oauth-pkce": "^0.0.7",
        "zod": "^3.25.51"
    }
}
