{
    "name": "medfetch",
    "version": "0.7.0",
    "type": "module",
    "description": "sql on fhir database runner implementation",
    "scripts": {
        "test": "vitest --browser=chromium",
        "build": "rm -rf dist && npx concurrently \"npx vite build\" \"npx tsc -p tsconfig.build.json && npx tsc-alias -p tsconfig.build.json\"",
        "unit": "vitest src",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "dev:codegen": "tsx src/bin/codegen.ts",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs && cp _headers ./docs/.vitepress/dist",
        "docs:preview": "vitepress preview docs"
    },
    "exports": {
        "./unpromisify": {
            "types": "./dist/unpromisify.d.ts",
            "import": "./dist/unpromisify.js",
            "node": "./dist/unpromisify.node.js",
            "default": "./dist/unpromisify.js"
        },
        "./sql": {
            "types": "./dist/sql.d.ts",
            "import": "./dist/sql.js",
            "default": "./dist/sql.js"
        },
        "./next": {
            "types": "./dist/next.d.ts",
            "import": "./dist/next.js",
            "default": "./dist/next.js"
        },
        "./sqlite-wasm": {
            "types": "./dist/sqlite-wasm.d.ts",
            "import": "./dist/sqlite-wasm.js",
            "default": "./dist/sqlite-wasm.js"
        },
        "./sqlite-wasm.worker": {
            "types": "./dist/sqlite-wasm.worker.d.ts",
            "import": "./dist/sqlite-wasm.worker.js",
            "default": "./dist/sqlite-wasm.worker.js"
        }
    },
    "files": [
        "dist",
        "src/**/*.ts",
        "tsconfig.json",
        "bin",
        "README.md"
    ],
    "keywords": [],
    "author": "",
    "license": "ISC",
    "peerDependencies": {
        "@sqlite.org/sqlite-wasm": "^3.49.1-build2",
        "kysely": "^0.28.2"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20250605.0",
        "@eslint/js": "^9.30.0",
        "@hono/vite-dev-server": "^0.19.1",
        "@tanstack/react-query": "^5.67.2",
        "@types/clarinet": "^0.12.3",
        "@types/fhir": "^0.0.41",
        "@types/json-schema": "^7.0.15",
        "@types/node": "^22.13.1",
        "@types/react": "18.2.0",
        "@types/react-dom": "^19.1.6",
        "@vitest/browser": "^3.2.4",
        "@vitest/coverage-istanbul": "3.1.2",
        "@vitest/coverage-v8": "2.1.9",
        "@vitest/web-worker": "^3.2.4",
        "concurrently": "^9.1.2",
        "eslint": "^9.30.1",
        "hono": "^4.7.11",
        "inquirer": "^12.6.3",
        "inquirer-autocomplete-standalone": "^0.8.1",
        "pagefind": "^1.3.0",
        "prettier": "^3.4.2",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "shiki": "^3.6.0",
        "ts-morph": "^25.0.0",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.16",
        "tsup": "^8.4.0",
        "tsx": "^4.19.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.35.1",
        "vite": "^6.2.2",
        "vitepress": "^1.6.3",
        "vitest": "^3.2.4",
        "vue": "^3.5.17",
        "wrangler": "^4.4.0"
    },
    "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228",
    "dependencies": {
        "@hono/node-server": "^1.14.4",
        "clarinet": "^0.12.6",
        "esm-env": "^1.2.2",
        "fflate": "^0.8.2",
        "jsonpointer": "^5.0.1",
        "kysely-generic-sqlite": "^1.2.1"
    }
}
